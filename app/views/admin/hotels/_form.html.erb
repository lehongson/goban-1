<div class="row">
  <div class="col-md-offset-3 col-md-6">
    <%= form_for [:admin, @distric] do |f| %>
      <%= render "shared/error_messages", object: @distric %>

      <%= f.label :name %><br/>
      <%= f.text_field :name, class: "form-control" %>
      
      <div class="actions">
        <%= f.submit "Submit", class: "btn-primary" %>
      </div>
    <% end %>
  </div>
</div>