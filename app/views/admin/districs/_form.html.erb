<div class="row">
  <div class="col-md-offset-3 col-md-6">
    <%= form_for [:admin, @distric] do |f| %>
      <%= render "shared/error_messages", object: @distric %>

      <%= f.label :provincial %><br/>
      <%= f.select :provincial_id, Provincial.all.collect { |p| [ p.name, p.id ] }, class: "form-control" %>

      <%= f.label :name %><br/>
      <%= f.text_field :name, class: "form-control" %>

      <%= f.label :latitude %><br/>
      <%= f.text_field :latitude, class: "form-control" %>

      <%= f.label :longitude %><br/>
      <%= f.text_field :longitude, class: "form-control" %>
      
      <div class="actions">
        <%= f.submit "Submit", class: "btn-primary" %>
      </div>
    <% end %>
  </div>
</div>